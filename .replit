modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_log_in_with_replit:1.0.0", "python_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["freetype", "glibcLocales"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 46009
externalPort = 3000
